<h1>Please enter a new product</h1>
<%= form_tag '/contacts', method: :post do %>
  <div>
    <%= label_tag :first_name, 'Enter First Name: ' %>
    <%= text_field_tag :first_name %>
  </div>
  <div>
    <%= label_tag :last_name, 'Enter Last Name: ' %>
    <%= text_field_tag :last_name %>
  </div>
  <div>
    <%= label_tag :email, 'Enter Email: ' %>
    <%= text_field_tag :email %>
  </div>
  <div>
    <%= label_tag :phone_number, 'Enter phone number: ' %>
    <%= text_field_tag :phone_number %>
  </div>
  <div>
    <%= submit_tag 'Submit!' %>
  </div>
<% end %> 

